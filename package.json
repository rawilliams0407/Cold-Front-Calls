{
  "name": "cold-front-calls",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "start": "run-p dev:*",
    "dev:eleventy": "eleventy --serve",
    "dev:tailwind": "npx @tailwindcss/cli -i src/assets/css/style.css -o _site/assets/css/style.css --watch",
    "dev:cms": "npx netlify-cms-proxy-server",
    "build": "run-s build:*",
    "build:eleventy": "npx @11ty/eleventy --config=eleventy.config.cjs",
    "build:tailwind": "npx @tailwindcss/cli -i src/assets/css/style.css -o _site/assets/css/style.css --minify",
    "preview": "eleventy --serve",
    "update-slideshow": "node scripts/update-slideshow.js"
  },
  "dependencies": {
    "@sanity/client": "^7.14.1",
    "@sanity/image-url": "^2.0.3",
    "@sanity/vision": "^5.8.1",
    "@stackbit/cms-sanity": "^0.2.92",
    "framer-motion": "^12.31.0",
    "lucide-react": "^0.563.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-router-dom": "^7.13.0",
    "sanity": "^5.7.0",
    "tailwindcss": "^4.1.18",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@tailwindcss/cli": "^4.1.18",
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "luxon": "^3.7.2",
    "markdown-it": "^14.1.0",
    "markdown-it-anchor": "^9.2.0",
    "npm-run-all": "^4.1.5",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  }
}